<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.axa.hackathon.axalink">

    <application
        android:allowBackup="true"
        android:icon="@drawable/image_appli"
        android:label="Sam"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BeginingActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FindedActivity" />
        <activity android:name=".NotFoundActivity" />
        <activity android:name=".HamburgerActivity" />
        <activity android:name=".AtoutDroneActivity" />
        <activity android:name=".ReclamationActivity"></activity>
    </application>

</manifest>